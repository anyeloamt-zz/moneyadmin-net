@model IEnumerable<MoneyAdmin.Data.Entities.Wallet>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<div>
    <ul>
        @foreach (var wallet in Model)
        {
            <li>
                @wallet.Name
                <br />
                @wallet.InitialBalance
            </li>
        }
    </ul>
</div>